{
  "name": "mock-server",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "g:public": "pnpm ts-node -r tsconfig-paths/register src/scripts/create-public-migration-module.ts",
    "g:tenant": "pnpm ts-node -r tsconfig-paths/register src/scripts/create-tenant-migration-module.ts",
    "r:public": "pnpm ts-node -r tsconfig-paths/register src/scripts/migrate-public.ts",
    "r:tenant": "pnpm ts-node -r tsconfig-paths/register src/scripts/migrate-tenant.ts",
    "r:all:tenants": "pnpm ts-node -r tsconfig-paths/register src/scripts/migrate-all-tenants.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0 --fix"
  },
  "keywords": [],
  "author": "Ferdinando A. Galera Jr",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "axios": "^1.13.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "redis": "^4.7.1",
    "typeorm": "^0.3.27",
    "uuidv7": "^1.1.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^17.2.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.1"
